programa {
  funcao inicio() {

    real N1,N2,soma,mult,div

    escreva("Primeiro número: ")
    leia(N1)
    escreva("Segundo número: ")
    leia(N2)

    soma = N1 + N2
    mult = N1*N2
    div = N1/N2

    escreva("Soma:", soma, "\n")
   
    escreva("Multiplicação:", mult, "\n")
   
    escreva("Divisão:", div)





    
  }
}
